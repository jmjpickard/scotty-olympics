# Prisma Database Connection (replace with your actual Supabase DB connection string)
DATABASE_URL="postgresql://postgres:[YOUR-DB-PASSWORD]@[YOUR-DB-HOST]:5432/postgres"

# Public Supabase Keys (safe to expose in browser)
NEXT_PUBLIC_SUPABASE_URL=YOUR_SUPABASE_URL
NEXT_PUBLIC_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Private Supabase Key (SERVER-SIDE ONLY)
SUPABASE_SERVICE_ROLE_KEY=YOUR_SECRET_SERVICE_ROLE_KEY

# Admin email for admin access
ADMIN_EMAIL=admin@example.com
NEXT_PUBLIC_ADMIN_EMAIL=admin@example.com

# Resend API key for sending emails
RESEND_API_KEY=your_resend_api_key
